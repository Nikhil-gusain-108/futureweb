{
    "version":2,
    "builds":[
        {"src":"futureEarnersWeb/asgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLamdaSize":"15mb",
            "runtime":"python3.10"
        }},
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "disDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"futureEarnersWeb/asgi.py"
        }
    ]
}